{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "concurrently \"tailwindcss -o ./app/styles/tailwind.css\" \"postcss styles --base styles --dir app/styles --env production\"",
    "dev": "concurrently \"npm run dev:css\" \"npm run dev:netlify\" \"remix watch\"",
    "dev:css": "concurrently \"postcss styles --base styles --dir app/styles -w\"",
    "postinstall": "remix setup node",
    "dev:netlify": "cross-env NODE_ENV=development netlify dev"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@kentcdodds/md-temp": "^1.1.0",
    "@netlify/functions": "^0.7.2",
    "@reach/menu-button": "^0.16.2",
    "@react-spring/web": "^9.3.1",
    "@remix-run/netlify": "^1.0.6",
    "@remix-run/react": "^1.0.6",
    "@tailwindcss/typography": "^0.4.1",
    "@types/marked": "^4.0.1",
    "clsx": "^1.1.1",
    "esbuild": "^0.12.9",
    "front-matter": "^4.0.2",
    "hamburger-react": "^2.4.1",
    "marked": "^4.0.5",
    "mdx-bundler": "^8.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.0.6",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.0.6",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/tailwindcss": "^2.2.4",
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.4",
    "postcss-cli": "^9.0.2",
    "tailwindcss": "^2.2.19",
    "ts-node": "^10.4.0",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  }
}
